<?xml version="1.0" encoding="utf-8" ?>
<configuration>

  <appSettings>
    <!--Browser can be CHROME or FIREFOX. Defaults to chrome of not set-->
    <add key="MAM_BROWSER" value="CHROME" />
    <!-- Set to LOCAL, SAUCELABS or PERFECTO. If running on cloud provider, Saucelabs or Perfecto settings are required. Defaults to local if not set -->
    <add key="MAM_MODE" value="LOCAL"/>
    <!-- Set to the server that will run MAM tests. Defaults to localhost if not specified -->
    <add key ="MAM_RUNON" value="127.0.0.1"/>
    
    <!--Device can be Android or IOS. Defaults to Android if not set-->
    <add key="MOBILE_DEVICE" value="ANDROID" />
    <!-- Set to LOCAL, SAUCELABS or PERFECTO. If set to anything other than local, Saucelabs or Perfecto settings are required. Defaults to local if not set -->
    <add key="MOBILE_MODE" value="PERFECTO"/>

    <!--TODO Perfecto settings. These settings will be read from the environmnet variables first, If not found will use the config file-->
    <add key="PERFECTO_SECURITYTOKEN" value="VAR_PERFECTO_SECURITYTOKEN"/>
    <add key="PERFECTO_HOST" value="mobilecloud.perfectomobile.com"/>
    <add key="PERFECTO_DEVICELIST" value=""/>
    
    <!--If running MAM or Mobile tests remotely on Saucelabs, specify Saucelabs settings-->
    <add key="SAUCELABS_REMOTESERVER" value=""/>
    <add key="SAUCELABS_USERNAME" value=""/>
    <add key="SAUCELABS_ACCESSKEY" value=""/>

    <!--Other configurations-->
    <add key="MAM_URL" value ="https://playermax-sit.aristocrat.systems"/>
    <add key="ANDROID_APK" value="https://build.phonegap.com/apps/3548965/download/android" />
    <add key="IOS_IPA" value="https://build.phonegap.com/apps/3548965/download/ios" />
   
  </appSettings>
</configuration>